<section class="w-fit mx-auto grid grid-cols-1 lg:grid-cols-3 md:grid-cols-2 justify-items-center justify-center gap-y-10 gap-x-14 mt-10 mb-5">
  <% products.forEach(function(product){ %>
    <div class="bg-white shadow-sm rounded-lg w-full">
      <img src="<%= product.image %>" alt="<%= product.name %>" class="w-full h-48 object-cover rounded-t-lg">
      <div class="p-4 flex justify-start items-start flex-col gap-2 h-64">
          <h3 class="text-lg font-bold text-gray-800">
              <%= product.name %>
          </h3>
          <p class="text-sm text-gray-500 mt-1">
              <%= product.category %>
          </p>
          <h4 class="text-red-600 font-semibold mt-2">
              <%= product.price.toFixed(2) %> AUD
          </h4>
          <p class="text-sm text-gray-800 mt-2 line-clamp-3">
              <%= product.description %>
          </p>
          <div class="ml-auto grow flex items-end text-gray-600"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-bag-plus" viewBox="0 0 16 16">
            <path fill-rule="evenodd" d="M8 7.5a.5.5 0 0 1 .5.5v1.5H10a.5.5 0 0 1 0 1H8.5V12a.5.5 0 0 1-1 0v-1.5H6a.5.5 0 0 1 0-1h1.5V8a.5.5 0 0 1 .5-.5z" />
            <path d="M8 1a2.5 2.5 0 0 1 2.5 2.5V4h-5v-.5A2.5 2.5 0 0 1 8 1zm3.5 3v-.5a3.5 3.5 0 1 0-7 0V4H1v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V4h-3.5zM2 5h12v9a1 1 0 0 1-1 1H3a1 1 0 0 1-1-1V5z" />
          </svg></div>
      </div>
  </div>
  <% }); %>
  

</section>

